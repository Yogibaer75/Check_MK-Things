#!/usr/bin/python
# -*- encoding: utf-8; py-indent-offset: 4 -*-

def parse_dell_powervault_me4(info):
    items = {
        "controllers": "durable-id",
        "pools": "name",
        "volumes": "durable-id",
        "fan": "durable-id",
        "enclosure-fru": "fru-location",
        "power-supplies": "durable-id",
        "sensors": "durable-id",
        "system": "system-name",
        "drives": "durable-id",
        "controller-statistics": "durable-id",
        "volume-statistics": "volume-name",
        "port": "durable-id",
    }
    parsed = {}
    import ast
    data = ast.literal_eval(info[0][0])

    for key in items:
        if data.get(key, False):
            status = data.get("status")
            elements = data.get(key)
            for element in elements:
                item = element.get(items[key])
                parsed.setdefault(item, element)

    return parsed

